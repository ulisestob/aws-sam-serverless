{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "module":"es2015",
    "target": "es2020",
    "sourceMap": false,
    "skipLibCheck": true,
    "esModuleInterop": true, 
    "moduleResolution":"node",
    "preserveConstEnums": true,
    "forceConsistentCasingInFileNames": true,
  },
  "exclude": ["node_modules", "**/*.test.ts"],
}